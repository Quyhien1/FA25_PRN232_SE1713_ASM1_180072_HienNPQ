@model OEMEVWarrantyManagementSystem.Repositories.HienNPQ.Models.BookingHienNpq

@{
    ViewData["Title"] = "Booking Details";
}

<h2>Booking Details</h2>

<dl class="row">
    <dt class="col-sm-3">ID</dt>
    <dd class="col-sm-9">@Model.BookingHienNpqid</dd>

    <dt class="col-sm-3">Station</dt>
    <dd class="col-sm-9">@Model.StationName</dd>

    <dt class="col-sm-3">Vehicle Type</dt>
    <dd class="col-sm-9">@Model.VehicleType</dd>

    <dt class="col-sm-3">Start Time</dt>
    <dd class="col-sm-9">@Model.StartTime</dd>

    <dt class="col-sm-3">End Time</dt>
    <dd class="col-sm-9">@Model.EndTime</dd>

    <dt class="col-sm-3">Price / Hour</dt>
    <dd class="col-sm-9">@Model.PricePerHour</dd>

    <dt class="col-sm-3">Status</dt>
    <dd class="col-sm-9">@Model.Status</dd>

    @if (Model.SupportInfoHienNpq != null)
    {
        <dt class="col-sm-3">License Plate</dt>
        <dd class="col-sm-9">@Model.SupportInfoHienNpq.LicensePlate</dd>
    }
</dl>

<p>
    <a asp-action="Edit" asp-route-id="@Model.BookingHienNpqid" class="btn btn-primary">Edit</a>
    <a asp-action="Index" class="btn btn-secondary">Back</a>
</p>
